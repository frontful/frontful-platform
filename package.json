  "devDependencies": {
    "babel-preset-frontful": "3.x",
    "eslint-config-frontful": "3.x",
    "mobx": "^4.x",
    "react": "^16.x",
    "react-dom": "16.x",
    "rimraf": "^2.6.2"
  }
