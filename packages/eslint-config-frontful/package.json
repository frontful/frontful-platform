{
  "name": "eslint-config-frontful",
  "version": "4.0.5",
  "private": true,
  "description": "Eslint configuration provider",
  "main": "./index.js",
  "scripts": {
    "build": "yarn test && frontful-tools build",
    "clean": "frontful-tools clean",
    "deploy": "yarn test && frontful-tools deploy",
    "test": "yarn test:eslint",
    "test:eslint": "eslint --ext .jsx,.js --max-warnings 0 .",
    "tools": "frontful-tools"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontful/eslint-config-frontful.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "contributors": [
    "Arturs Girons <arturs.girons@gmail.com> (https://artursgirons.info/)"
  ],
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.0",
    "frontful-config": "4.x",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "frontful-tools": "4.x"
  }
}
