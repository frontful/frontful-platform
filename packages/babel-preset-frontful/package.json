{
  "name": "babel-preset-frontful",
  "version": "4.0.6",
  "private": true,
  "description": "Babel preset provider and package deployment utilities",
  "main": "./index.js",
  "scripts": {
    "build": "yarn test && frontful-tools build",
    "clean": "frontful-tools clean",
    "deploy": "yarn test && frontful-tools deploy",
    "test": "yarn test:eslint",
    "test:eslint": "eslint --ext .jsx,.js --max-warnings 0 .",
    "tools": "frontful-tools"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontful/frontful-platform.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "contributors": [
    "Arturs Girons <arturs.girons@gmail.com> (https://artursgirons.info/)"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.4.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "frontful-common": "4.x",
    "frontful-config": "4.x",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "frontful-tools": "4.x"
  }
}
