{
  "name": "frontful-style",
  "version": "4.0.4",
  "private": true,
  "description": "CSS-in-JS styling approach for component based applications with focus on style API-ification, dynamic runtime modifications and isomorphism without compromising CSS features",
  "main": "./style/index.js",
  "scripts": {
    "build": "yarn test && frontful-tools build",
    "clean": "frontful-tools clean",
    "deploy": "yarn test && frontful-tools deploy",
    "test": "yarn test:eslint",
    "test:eslint": "eslint --ext .jsx,.js --max-warnings 0 .",
    "tools": "frontful-tools"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontful/frontful-platform.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "contributors": [
    "Arturs Girons <arturs.girons@gmail.com> (https://artursgirons.info/)"
  ],
  "dependencies": {
    "frontful-utils": "4.x",
    "inline-style-prefixer": "^5.0.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "frontful-tools": "4.x"
  }
}
